<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
   

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="Widget.ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse" />

    <style name="OneWestTheme" parent="Theme.Sherlock.Light.DarkActionBar">
        <item name="homeAsUpIndicator">@drawable/new_indicator</item>
        <item name="android:homeAsUpIndicator">@drawable/new_indicator</item>
    </style> 
    
    <style name="OneWestFlatTheme" parent="Theme.Sherlock.Light.DarkActionBar">
        <item name="homeAsUpIndicator">@drawable/new_indicator</item>
        <item name="android:homeAsUpIndicator">@drawable/new_indicator</item>
        <item name="android:actionBarStyle">@style/Widget.ActionBar.FlatRed</item>   
    </style> 
    
    <style name="Widget.ActionBar.FlatRed">
        <item name="android:background">@color/flatred</item>
        <item name="android:icon">@android:color/transparent</item>
    </style>
    

</resources>
